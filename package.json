{
  "name": "image-api",
  "version": "1.0.0",
  "description": "Express + TypeScript image resizing API (Sharp) with caching and tests",
  "main": "dist/src/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/src/index.js",
    "lint": "eslint --ext .ts src",
    "prettier": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "test": "jasmine --config=jasmine.json"
  },
  "author": "Ali Odeh",
  "license": "MIT",
  "dependencies": {
    "csvtojson": "^2.0.10",
    "express": "^5.1.0",
    "jasmine": "^5.10.0",
    "jasmine-spec-reporter": "^7.0.0",
    "sharp": "^0.34.3",
    "supertest": "^7.1.4"
  },
  "devDependencies": {
    "@types/csvtojson": "^1.1.5",
    "@types/express": "^5.0.3",
    "@types/jasmine": "^5.1.9",
    "@types/node": "^24.3.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^3.1.10",
    "prettier": "^2.5.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  },
  "directories": {
    "test": "tests"
  },
  "keywords": []
}
